{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "angular-elements-router": {
      "projectType": "library",
      "root": "projects/angular-elements-router",
      "sourceRoot": "projects/angular-elements-router/src",
      "prefix": "aer",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/angular-elements-router/tsconfig.lib.json",
            "project": "projects/angular-elements-router/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/angular-elements-router/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {}
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/angular-elements-router/tsconfig.lib.json",
              "projects/angular-elements-router/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "example-micro-frontend": {
      "projectType": "application",
      "root": "projects/example-micro-frontend",
      "sourceRoot": "projects/example-micro-frontend/src",
      "prefix": "mf",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:build",
          "options": {
            "singleBundle": true,
            "outputPath": "dist/example-micro-frontend",
            "index": "projects/example-micro-frontend/src/index.html",
            "main": "projects/example-micro-frontend/src/main.ts",
            "polyfills": "projects/example-micro-frontend/src/polyfills.ts",
            "tsConfig": "projects/example-micro-frontend/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/example-micro-frontend/src/favicon.ico",
              "projects/example-micro-frontend/src/assets"
            ],
            "styles": [
              "projects/example-micro-frontend/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/example-micro-frontend/src/environments/environment.ts",
                  "with": "projects/example-micro-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "example-micro-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "example-micro-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "example-micro-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {}
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/example-micro-frontend/tsconfig.app.json",
              "projects/example-micro-frontend/tsconfig.spec.json",
              "projects/example-micro-frontend/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "example-platform": {
      "projectType": "application",
      "root": "projects/example-platform",
      "sourceRoot": "projects/example-platform/src",
      "prefix": "aer",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/example-platform",
            "index": "projects/example-platform/src/index.html",
            "main": "projects/example-platform/src/main.ts",
            "polyfills": "projects/example-platform/src/polyfills.ts",
            "tsConfig": "projects/example-platform/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/example-platform/src/favicon.ico",
              "projects/example-platform/src/assets"
            ],
            "styles": [
              "projects/example-platform/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/example-platform/src/environments/environment.ts",
                  "with": "projects/example-platform/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "example-platform:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "example-platform:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "example-platform:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {}
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/example-platform/tsconfig.app.json",
              "projects/example-platform/tsconfig.spec.json",
              "projects/example-platform/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "angular-elements-router"
}
